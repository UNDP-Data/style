    "url": "https://github.com/UNDP-Data/style/issues"
  },
  "homepage": "https://github.com/UNDP-Data/style#readme",
  "dependencies": {
    "@mapbox/spritezero": "^8.0.0",
    "@unvt/charites": "^0.1.4",
    "fs": "0.0.1-security",
    "glog": "^1.7.0",
    "path": "^0.12.7",
    "rimraf": "^3.0.2"
  }
}
