{
  "name": "style",
  "version": "1.0.0",
  "description": "This repository is to manage the style.json for base map of geohub",
  "main": "index.js",
  "directories": {
    "doc": "docs"
  },
  "scripts": {
    "build": "npm run build:style && npm run build:sprite",
